<h2>Listing Plans</h2>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
<%= for plan <- @plans do %>
    <tr>
      <td><%= plan.name %></td>
      <td class="text-right">
        <span><%= link "Show", to: plan_path(@conn, :show, plan), class: "btn btn-default btn-xs"%></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>
